// Place your key bindings in this file to override the defaults
[
  {
    "key": "cmd+d",
    "command": "-editor.action.addSelectionToNextFindMatch",
    "when": "editorFocus"
  },
  {
    "key": "cmd+d",
    "command": "extension.caseSensitiveAddNext",
    "when": "editorFocus"
  },
  {
    "key": "ctrl+shift+d",
    "command": "-extension.caseSensitiveAddNext",
    "when": "editorFocus"
  }
]
